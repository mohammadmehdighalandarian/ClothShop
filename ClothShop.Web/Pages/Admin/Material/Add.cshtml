@page
@using ClothShop.DataLayer.Entities.Product.ProductDetails
@model ClothShop.Web.Pages.Admin.Material.AddModel
@{
    ViewData["Title"] = "افزودن جنس ";
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">افزودن جنس  جدید</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <form method="post" enctype="multipart/form-data">
        <div class="col-md-8">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    جنس 
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="form-group">
                        <label>عنوان جنس </label>
                        <input type="text" asp-for="Material.MaterialName" class="form-control">
                    </div>
                    <input type="submit" value="ذخیره اطلاعات" class="btn btn-success" />
                </div>
                <!-- /.panel-body -->
            </div>
        </div>

    </form>
</div>


